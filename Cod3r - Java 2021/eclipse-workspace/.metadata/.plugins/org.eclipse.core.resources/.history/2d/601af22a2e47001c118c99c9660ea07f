package controle;

import java.util.Locale;
import java.util.Scanner;

public class DoWhile {
	
	public static void main(String[] args) {
		
		// if(...) sentença; ou {}
		// while(...) sentença; ou {}
		// for (..;..;..) sentença; ou {}
		// do sentença; ou {} while (...);
		
		Scanner entrada = new Scanner(System.in).useLocale(Locale.US);

		do {
			
		} while (false);

		entrada.close();
		
	}

}
